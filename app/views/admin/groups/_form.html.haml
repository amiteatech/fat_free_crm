.section
  %table
    %tr
      %td
        .label.top.req #{t :name}:
        = f.text_field :name, :required => true
    %tr
      %td
        .label.top.req #{t :users}:
        %br
        = f.select :user_ids, User.where(company_id: current_user.company_id).map {|u| [u.full_name, u.id]}, {}, :multiple => true, :class => 'select2', :required => true
