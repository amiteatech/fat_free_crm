<p id="notice"><%= notice %></p>

<h1>Access On Forms</h1>

<table>
  <thead>
    <tr>
      <th>School form</th>
      <th>Company</th>
      <th>Users</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @access_on_forms.each do |access_on_form| %>
      <tr>
        <td><%= access_on_form.school_form_id %></td>
        <td><%= access_on_form.company_id %></td>
        <td><%= access_on_form.users_id %></td>
        <td><%= link_to 'Show', access_on_form %></td>
        <td><%= link_to 'Edit', edit_access_on_form_path(access_on_form) %></td>
        <td><%= link_to 'Destroy', access_on_form, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Access On Form', new_access_on_form_path %>
